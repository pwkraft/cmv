@Article{valentino2011election,
  author    = {Valentino, N.A. and Brader, T. and Groenendyk, E.W. and Gregorowicz, K. and Hutchings, V.L.},
  journal   = {Journal of Politics},
  title     = {Election night's alright for fighting: The role of emotions in political participation},
  year      = {2011},
  number    = {1},
  pages     = {156--70},
  volume    = {73},
  comment   = {- effect of specific emotions on political participation: anger, anxiety, enthusiasm
- theoretical argument based on cognitive appraisal theory and affective intelligence model

cognitive appraisal theory: individuals take deliberate actions to cope with their emotions
