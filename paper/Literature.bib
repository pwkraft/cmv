  groups    = {Knowledge / Sophistication, 346 - Political Psychology, [patrick:], Public Opinion - General, PolPsych Overview},
  keywords  = {03) Ideology + 02) Attitudes and Belief Systems + 04) Partisan Identification and Ideology; knowledge},
  owner     = {patrick},
  publisher = {JSTOR},
  timestamp = {2018.05.29},
}

