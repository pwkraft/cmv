@Article{Rodden2011,
  author         = {Rodden, J. and Wibbels, E.},
  title          = {Dual Accountability and the Nationalization of Party Competition: Evidence from Four Federations},
  journal        = {Party Politics},
  year           = {2011},
  volume         = {17},
