@Article{blair2019declaring,
  author    = {Blair, Graeme and Cooper, Jasper and Coppock, Alexander and Humphreys, Macartan},
  journal   = {American Political Science Review},
  title     = {Declaring and diagnosing research designs},
  year      = {2019},
  number    = {3},
  pages     = {838--859},
  volume    = {113},
  comment   = {overview:
- framework (and software) to declare research designs prior to collecting the data or conducting the analysis
- MIDA: causal Model, Inquiry, Data strategy, Answer strategy
- diagnostic statistic: a summary statistic generated from a run of a design (e.g., difference between estimated and actual average treatement effect, or significance at 5% level)
- diagnosand: summary of the distribution of diagnostic statistics (e.g., expected bias etc.)
- authors discuss several possible diagnosands (beyond power!), their usefulness depends on the respective research question etc.
- a design is "diagnosand-complete" for a given diagnosand if that diagnosand can be calculated from the declared design
- interesting example: a 3 arm design can be better than a 2X2 if the interaction effect is relatively weak and the researcher is more interested in the main effects
- "Declaration of these features in code provides sufﬁcient information for researchers and readers to use Monte Carlo techniques to diagnose properties such as power, bias, accuracy of qualitative causal inferences, and other 'diagnosands.'"

questions/comments
- appendix including code examples is linked on JabRef as well
- also check https://declaredesign.org/ for design examples and code snippets},
  file      = {:pdfs/blair2019declaring.pdf:PDF;:pdfs/blair2019appendix.pdf:PDF},
  groups    = {Experiments, patrick:4, Power Analysis & Preregistration},
  publisher = {Cambridge University Press},
}

@Article{benedictis2019persuading,
  author    = {De Benedictis-Kessner, Justin and Baum, Matthew A and Berinsky, Adam J and Yamamoto, Teppei},
  journal   = {American Political Science Review},
  title     = {Persuading the Enemy: Estimating the Persuasive Effects of Partisan Media with the Preference-Incorporating Choice and Assignment Design},
  year      = {2019},
  number    = {4},
  pages     = {902--916},
  volume    = {113},
  comment   = {overview:
- question: is polarization a cause of media fragmentation or rather an effect fo media fragmentation? Causal direction is not completely clear in the literature
- major issue in past research: unable to disentangle the effect of selective exposure and actual persuasion
- Preference-Incorporating Choice and Assignment (PICA) design, includes free choice and forced exposure of partisan media sources (Fox News, MSNBC, entertainment)
- design allows authors to estimate the degree of polarization caused by selective exposure and the persuasive effect of partisan media, as well as sensitivityanalyses accounting for discrepancies between stated preferences and actual choice
- exposire to more partisan news can sway opinions of even those respondents who are already exposed to some degree of partisan news, but they are most effective among respondents who usually prefer entertainment rather than partisan news
- "We ﬁnd that partisan media can polarize both its regular consumers and inadvertent audiences who would otherwise not consume it, but ideologically opposing media potentially also can ameliorate the existing polarization between consumers."
-> polarization is not simply a function of selective exposure, persuasion effects play an important role as well

comments/questions:
- interesting and useful article that can serve as a template for future persuasion/exposure studies
- design is almost the same as in our lab project implementing a randomized exposure + endogenous exposure design, but we keep the actual content of the news article constant, which could be an interesting extension
- one of their outcome measures is intention to share the news article, this could also be useful for the lab project},
  file      = {:pdfs/benedictis2019persuading.pdf:PDF},
  groups    = {Media Exposure, patrick:5, Selective Exposure},
  publisher = {Cambridge University Press},
}

@Article{hatemi2019ideology,
  author    = {Hatemi, Peter K and Crabtree, Charles and Smith, Kevin B},
  journal   = {American Journal of Political Science},
  title     = {Ideology Justifies Morality: Political Beliefs Predict Moral Foundations},
  year      = {2019},
  number    = {4},
  pages     = {788--806},
  volume    = {63},
  comment   = {overview:
- Q: do moral intuitions drive political beliefs or do political beliefs drive moral intuitions?
- A: empirical evidence contradicts MFT, idology is causally prior to moral intuitions
- data: Australian panel used by Smith et al. 2017, MTurk panel (2014-2018), ANES 2008-2009 panel

analyses:
1) comparing stability of MFQ and ideology using different measurment approaches: ideology is more stable regardless of measurement approach
2) cross-lagged panel analysis (akin to testing Granger causality): lagged ideology is predictive of moral foundations (controlling for lagged moral foundations), but not vice versa
3) random forest analysis, comparing predictive importance: ideology higher predictive importance for moral foundations than vice versa

questions/comments:
- overall good review of the debate, includes some useful citations
- good discussion of the extent to which the analyses indeed show causality (especially in conclusion) and to what extent there are still limitations (e.g., because they cannot rule out spuriousness)
- Ultimately, this comes down to an argument about measurement. From a pure theoretical perspective, it seems more plausible that moral considerations are causally prior to ideology and political beliefs (think about child socialization etc.). However, the evidence definitely suggests that ideology measures are more stable and predictive of moral foundations. The question is whether we should draw a conclusion about MFQ as a measurement or about morality in general? I agree regarding the former but I'm not quite convinced regarding the latter.},
  file      = {:pdfs/hatemi2019ideology.pdf:PDF},
  groups    = {Ideology, Values, 346 - Political Psychology, [patrick:], Morality},
  publisher = {Wiley Online Library},
}

