@Article{knox2019design,
  author    = {Knox, Dean and Yamamoto, Teppei and Baum, Matthew A and Berinsky, Adam J},
  journal   = {Journal of the American Statistical Association},
  title     = {Design, identification, and sensitivity analysis for patient preference trials},
  year      = {2019},
  number    = {528},
  pages     = {1532--1546},
  volume    = {114},
  comment   = {overview:
- proposes a general design for patient preference trials (PPTs) that involves a combination of forced exposure and free choice condition for media exposure experiments
- motivation: "If a treatment has different effects on those who would choose to take it and those who would not, the average treatment effect estimated in a standard randomized controlled trial (RCT) may give a misleading picture of its impact outside of the study sample."
- key causal quantity: ACTE: average choice-specific treatment effect, represents the conditional average treatment effect for the sub-population of subjects who would choose a particular treatment option
- ACTE is identified under (1) randomization of designs, (2) randomization of the forced treatment, and (3) mean ignorability of measurement error (i.e., the discrepancy between stated and revealed preferences is ignorable / not correlated with background characteristics that are associated with potential outcomes
- Assumption 3 is key (and strong in certain contexts), the framework allows for sensitivity analysis of its violation

questions/comments:
- no-design-effect assumption is important, our lab project essentially argues that a nominally identical treatment has different effects on the outcome for the same unit depending on whether the treatment was randomly assigned in the forced exposure condition or voluntary chosen in the free-choice condition -> Essentially we are arguing that this key assumption is violated, which would be an interesting methodological contribution.},
  file      = {:pdfs/knox2019design.pdf:PDF},
  groups    = {Media Exposure, patrick:1, Selective Exposure},
  publisher = {Taylor \& Francis},
}

@Article{rodman2019timely,
  author    = {Rodman, Emma},
  journal   = {Political Analysis},
  title     = {A Timely Intervention: Tracking the Changing Meanings of Political Concepts with Word Vectors},
  year      = {2020},
  number    = {1},
  pages     = {87--111},
  volume    = {28},
  comment   = {overview:
- basic intro to word vectors & application that shows how to use word2vec results to track the change of the cultural meaning of words (semantic change) over time
- challenges for diachronic analysis: arbitrary cut points, language instability, spatial noncompatibility
- tests four time-sensitive implementations of word2vec: naive time series model, overlapping time series model, chronologically trained model, aligned time series model -> chronologically trained model performs best (using supervised topic model as gold standard)
- suggests best practices for using word2vec to study small corpora for time series questions: bootstrap resampling of documents, selective language stabilization & stemming, and chronological training

comments/questions:
- great intro, explains the underlying logic behind word2vec in a way that is approachable for less technical audience},
  file      = {:pdfs/rodman2019timely.pdf:PDF},
  groups    = {Psychology of Word Use, Text Analysis, patrick:4, Word Embeddings},
  publisher = {Cambridge University Press},
}

@Article{rheault2019word,
  author    = {Rheault, Ludovic and Cochrane, Christopher},
  journal   = {Political Analysis},
  title     = {Word Embeddings for the Analysis of Ideological Placement in Parliamentary Corpora},
  year      = {2020},
  number    = {1},
  pages     = {112--133},
  volume    = {28},
  comment   = {overview:
- introduces word embeddings augmented with political metadata to score parties based on parliamentary speeches -> party embeddings
- party embeddings are succesful at capturing latent concepts such as ideology
- data: parliamentary debates in the United States, Britain, and Canada

advantages of embedding models over scaling based on pure word frequency (e.g., wordscore, wordfish):
- can account for the evolution of language over time
- allows to map political actors in a multi-dimensional space
- ability to include control variables is useful to account for the effect of institutional characteristics
- words and phrases closest to the location of the party can help researchers to interpret the meaning of each axis.

questions/comments:
- does the neural network automatically take into account potential interactions between party indicator variables and the context words?
- interesting text preprocessing step: subsampling, i.e., the random removal of frequent words from the context window during estimation, achieves a goal similar to stopword removal by limiting the influence of overly frequent tokens
- second dimension in US is interpreted as north-south realignment (p.10), but the timeline doesn't really match up all that well, the reversal comes way too late.},
  file      = {:pdfs/rheault2019word.pdf:PDF},
  groups    = {Ideology, Text Analysis, patrick:4, Word Embeddings},
  publisher = {Cambridge University Press},
}

@InProceedings{rezapour2019enhancing,
  author    = {Rezapour, Rezvaneh and Shah, Saumil H and Diesner, Jana},
  booktitle = {Proceedings of the Tenth Workshop on Computational Approaches to Subjectivity, Sentiment and Social Media Analysis},
  title     = {Enhancing the Measurement of Social Effects by Capturing Morality},
  year      = {2019},
  pages     = {35--45},
  comment   = {overview:
- paper expands the MFT dictionary from 324 to 4,636 syntactically disambiguated entries and shows that it improves predictions with 2 benchmark data sets (semeval 2016 Stance detection benchmark dataset & Baltimore protest benchmark dataset)

procedure to expand MFT dictionary:
1) sort words in misc category to virtues and vices
2) manually annotate each entry with one or more best fitting parts of speech (POS) tags
3) add variations of the original words and sense, such as grammatical inflections
4) add synonyms, antonyms, and direct hypernyms of all original entries using WordNet
5) make word to category assignment exclusive by assigning each word to the best fitting category
6) expand nouns with their plural r singular form, adjectives with comparatives and superlative, and lammatized the verbs

questions/comments:
- enhanced morality lexicon is freely available at: https://doi.org/10.13012/B2IDB-3805242_V1
- interesting expansion of MFT dictionary, but I'm not sure if it's worth examining it in much more detail as it was not driven by theory and the improved predictive accuracy does not necessarily imply higher measurement validity},
  file      = {:pdfs/rezapour2019enhancing.pdf:PDF},
  groups    = {Text Analysis, patrick:4, Morality},
}

