@Article{niedenthal1999emotional,
  author    = {Niedenthal, Paula M. and Halberstadt, Jamin B. and Innes-Ker, {\AA}se H.},
  journal   = {Psychological Review},
  title     = {Emotional response categorization.},
  year      = {1999},
