@Article{strahan2002subliminal,
  author    = {Strahan, Erin J and Spencer, Steven J and Zanna, Mark P},
  journal   = {Journal of Experimental Social Psychology},
  title     = {Subliminal priming and persuasion: Striking while the iron is hot},
  year      = {2002},
  number    = {6},
  pages     = {556--568},
  volume    = {38},
  comment   = {Objective: When does subliminal priming lead to behavioral change?

Experiment 1:
- Participants (Ps) completed a ‘marketing study’ in which they evaluate food products
1) All Ps given cookies, half of Ps were given a glass of water after the cookies
2) All Ps completed a lexical decision task, half received thirst-related primes, half received neutral primes
3) All Ps completed a taste test of a mystery drink
4) Researchers measured the amount of liquid consumed as the DV [Multiple explicit measures of thirst in both Experiment 1 and 2 served as manipulation checks]
- Findings: Ps who were thirsty and received subliminal thirst primes consumed the most liquid. Ps who were primed with thirst words but were physically satiated drank nearly equally as much as those satiated Ps who were primed with neutral words. 

Experiment 2:
- Same pretense as Experiment 1
1) Ps asked to evaluate two sports drinks, SuperQuencher and PowerPro [Pilot studies confirmed SuperQuencher is generally perceived as more thirst quenching]
2) Half of Ps received thirst-related primes
3) Ps asked to select number of coupons for either sports drink
- Findings: Ps who received thirst-related primes were more favorable of SuperQuencher. Ps who received thirst-related primes asked for more SuperQuencher coupons than for PowerPro coupons. 
* Follow-up study also revealed a preference for SuperQuencher over PowerPro for both Ps who were subliminally primed with thirst words and for Ps who were supraliminally presented.

Experiment 3:
- Ps were told they would be completing a marketing study which also a decision making task. Half of Ps were told the decision making task would either be conducted alone or with another person.
1) Ps completed a lexical decision task, half received sad face primes
2) Ps were told to evaluate two CDs, Tweed Monkeys (which the ad claims will put you in a good mood) or Crystal Hammer (which the ad claims has a strong sound) [Pilot studies confirmed Tweed Monkeys CD was more likely to restore positive mood]
3) Ps were allowed seven songs total to listen to from either album
- Findings: Researchers found a significant three way interaction. Ps who received sad face primes (and hence lowered their positive mood [Mood measured implicitly through an evaluation of a sad piece of music, which seems questionable]) were more likely to rate Tweed Monkeys as more favorable only if they expected to interact with another person on the decision making task. Those Ps who received sad face primes but did not expect to interact with another person did not rate Tweed Monkeys as more favorable. Ps who received sad face primes and who expected to interact with someone also selected more Tweed Monkeys songs to listen to.

Take home message: Priming is not identical across situations. One needs the prime AND the motivation/outlet in order for the prime to have any meaningful impact. In other words, primes need to be accessible and applicable in order to have an influence on behavior.

Class discussion: The authors use this as evidence that individuals can be exploited by subliminal priming techniques. Agree?},
  file      = {strahan2002subliminal.pdf:pdfs/strahan2002subliminal.pdf:PDF},
  groups    = {Automaticity, Political Cognition, Advances Paper, patrick:5},
  keywords  = {07) Automaticity + Automaticity and priming},
  owner     = {patrick},
  publisher = {Elsevier},
  timestamp = {2018.05.29},
}

@Article{Stroud2007,
