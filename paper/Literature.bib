@Article{blackwell2013framework,
  author    = {Blackwell, Matthew},
  journal   = {American Journal of Political Science},
  title     = {A framework for dynamic causal inference in political science},
  year      = {2013},
  number    = {2},
  pages     = {504--520},
  volume    = {57},
  comment   = {overview:
- traditional (single-shot) causal inference methods are insufficient to capture dynamic processes because they essentially assume that decisions are made all at once, which "forces a choice between omitted variable bias and posttreatment bias"
- issue of time-varying confounders that affect future treatment, is affected by past treatment, and is correlated with the outcome -> controlling for it leads to posttreatment bias and not controlling for it leads to omitted variable bias
- dynamic causal inference captures the effect of a whole action history (i.e., history of past treatment values) rather than the effect of an individual treatment at time t
- basic method relies on inverse probability of treatment weigting (IPTW) combined with marginal structural models (MSM), the weighting creates a pseudosample where dynamic selection is eliminated and the MSM "dictates the form of the relationship between the large number of possible action histories and the outcome and serve to reduce the number of causal parameters"
- method requires strong assumptions (sequential ignorability & positivity/common support) and develops a diagnostic tool (history-adjusted balance) as well as a sensitivity-analysis framework to address potential causes of concern

questions/comments
- method is very interesting but there are a couple of reasons why it probably cannot directly be used for the CCRB project with Ben. The first reason is that treatements are assumed to be binary (although an extension to continuous treatments may be possible). More importantly, the method assumes that there is a single outcome at the end of the time period and we are estimating the effect of different action/treatment histories. In our case, however, we have outcome measures at each time point, which makes the whole weighting strategy more complicated (and I am not quite sure if it is actually feasible).},
  file      = {blackwell2013framework.pdf:pdfs/blackwell2013framework.pdf:PDF},
  groups    = {Time Series, Causal Inference, patrick:3},
  owner     = {patrick},
  publisher = {Wiley Online Library},
  timestamp = {2014.09.10},
}

@Article{blackwell2014selection,
  author    = {Blackwell, Matthew},
  journal   = {Political Analysis},
  title     = {A Selection Bias Approach to Sensitivity Analysis for Causal Effects},
  year      = {2014},
  number    = {2},
  pages     = {169-182},
  volume    = {22},
  abstract  = {The estimation of causal effects has a revered place in all fields of empirical political science, but a large volume of methodological and applied work ignores a fundamental fact: most people are skeptical of estimated causal effects. In particular, researchers are often worried about the assumption of no omitted variables or no unmeasured confounders. This article combines two approaches to sensitivity analysis to provide researchers with a tool to investigate how specific violations of no omitted variables alter their estimates. This approach can help researchers determine which narratives imply weaker results and which actually strengthen their claims. This gives researchers and critics a reasoned and quantitative approach to assessing the plausibility of causal effects. To demonstrate the approach, I present applications to three causal inference estimation strategies: regression, matching, and weighting.},
  doi       = {10.1093/pan/mpt006},
  eprint    = {http://pan.oxfordjournals.org/content/22/2/169.full.pdf+html},
  file      = {blackwell2014selection.pdf:pdfs/blackwell2014selection.pdf:PDF},
  groups    = {Causal Inference, patrick:1},
  keywords  = {Matching Innovations},
  owner     = {patrick},
  timestamp = {2014.11.15},
  url       = {http://pan.oxfordjournals.org/content/22/2/169.abstract},
}

@Unpublished{blackwell2011multiple,
  author    = {Matthew Blackwell and James Honaker and Gary King},
  title     = {Multiple Overimputation: A Unified Approach to Measurement Error and Missing Data},
  note      = {Working Paper, Copy at http://j.mp/jqdj72},
  month     = {10/2010},
  year      = {2011},
  abstract  = {<p>Social scientists typically devote considerable effort to reducing measurement error during data collection and then ignore the issue during data analysis. Although many statistical methods have been proposed for reducing measurement error-induced biases, few have been widely used because of implausible assumptions, high levels of model dependence, difficult computation, or inapplicability with multiple mismeasured variables. We develop an easy-to-use alternative that generalizes the popular multiple imputation (MI) framework by treating missing data problems as a special case of extreme measurement error and correcting for both. Like MI, the proposed "multiple overimputation" (MO) framework is a simple two-step procedure. First, multiple (ca. 5) completed copies of the data set are created where cells measured without error are held constant, those missing are imputed from the distribution of predicted values, and cells (or entire variables) with measurement error are "overimputed," that is imputed from a predictive distribution with observation-level priors defined by the mismeasured values and available external information, if any. In the second step, analysts can then run whatever statistical method they would have run on each of the overimputed data sets as if there had been no missingness or measurement error; the results are then combined via a simple procedure. We also (will) offer open source software that implements all the methods described herein.</p>},
  file      = {blackwell2011multiple.pdf:pdfs/blackwell2011multiple.pdf:PDF},
  groups    = {Missing Data},
  owner     = {patrick},
  timestamp = {2014.01.13},
}

