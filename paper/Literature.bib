@Article{tibshirani1996regression,
  author    = {Tibshirani, Robert},
  title     = {Regression shrinkage and selection via the lasso},
  journal   = {Journal of the Royal Statistical Society. Series B (Methodological)},
  year      = {1996},
  volume    = {58},
  number    = {1},
  pages     = {267--288},
  comment   = {overview:
- foundation of lasso, minimizes the residual sum of squares subject tot the sum of the absolute value of the coefficients being less than a constant
- tends to produce some coefficients that are exactly 0 and therefore gives interpretable models (in contrast to ridge regression)
- tuning parameter can be chosen via k-fold cross-validation, standard errors can be obtained via bootstrap
- advantages over subset selection: more stable, better prediction accuracy
- advantages over ridge regression: ridge is more stable, but it doesn't reduce parameters to 0 which makes it less interpretable 
- the lasso can be conceptualized as a Bayes posterior mode under independent double-exponential priors for the beta_js.

comparison of methods based on simulations:
a) small number of large effects: subset selection does best, the lasso not quite as well and ridge regression does quite poorly
b) small to moderate number of moderate-sized effects: the lasso does best, followed by ridge regression and then subset selection
c) large number of small effects: ridge regression does best by a good margin, followed by the lasso and then subset selection

questions for Nick's application:
- data standardized?
- cross-validation to select tuning parameter?
- maybe ridge regression would be better because it performs better with a large number of small effects},
  file      = {tibshirani1996regression.pdf:pdfs/tibshirani1996regression.pdf:PDF},
  groups    = {Regression, Machine Learning},
  owner     = {patrick},
  publisher = {JSTOR},
  timestamp = {2014.04.02},
}

